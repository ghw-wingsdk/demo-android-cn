    package="com.wa.sdk.cn.demo">

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- 华为 SDK START -->
        <!-- 华为 app id -->
        <meta-data
            android:name="com.wa.sdk.HUAWEI_APP_ID"
